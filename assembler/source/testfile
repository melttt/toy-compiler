;qweiouyqwiueyqwiue
;--------------全局变量---------------
    var Str
    var GlobalVar
    var GlobalArray [ 256]   ;sdajlaskdjlaks
;-------------123123123
SETSTACKSIZE 4096
        Func MyAdd
{
    ;sadf
    Param Y
    Param X
    ; jububian
    Var Sum
    Mov Sum, X
    Add Sum, Y
    ;sfdsf
    Mov _RetVal, Sum
    ;add ret by auto
}

;dsf
Func MyFunc
{
    Var MySum
        Push 16
        Push 32
        Call MyAdd
        Mov Mysum, _RetVal
        Mul MySum, 2
        Mov GlobalVar, MySum
        ;sfddfsdfdf
        Mov GlobalArray [ 0 ], "This"
        Mov GlobalArray [ 1 ], "is"
        Mov GlobalArray [ 2 ], "an"
        Mov GlobalArray [ 3 ], "array"

        Mov GlobalArray [ 4 ], Str
}


Func _Main{
    var aa
    var b
    Mov aa, 1
    Mov b, 2
    jg b, aa, XXX
XXX:
    CallHost OKOK
    Call MyFunc
}
